            <res>
                <js>fuelux.wizard.min.js,json2.js</js>
                <css></css>
            </res>
            <ismenu>1</ismenu>
        </operation>
        
        <operation>
            <path>Admin/add</path>
            <name>PLG_COLLECTION_ADD</name>
            <menu_group>5</menu_group>
